---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Ceyleen's Computer Science Blog</title>
	<link href="https://ceyleenp1486.github.io/blog/atom.xml" rel="self"/>
	<link href="https://ceyleenp1486.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://ceyleenp1486.github.io/Blog</id>
	<author>
		<name>Ceyleen Paulino</name>
		<email>ceyleenp1486@afsenyc.org</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://ceyleenp1486.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://ceyleenp1486.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
